{
	"id": "contactsApp",
	"loaderConfig": {
		"paths": {
			"contactsApp": "../demo"
		}
	},
	"dependencies": [
			"dojo/store/Observable",
			"dojox/mobile/Heading",
			"dojox/mobile/ScrollableView",
			"dojox/mobile/ToolBarButton"
	],
	"modules": [
	],
	"controllers": [
		"dojox/app/controllers/Load",
		"dojox/app/controllers/Transition",
		"dojox/app/controllers/Layout"
	],
	"nls": "demo/nls/contacts",
	"defaultTransition": "slide",
	"defaultView": "list",
	"stores": {
		"contacts": {
			"type": "dojo/store/Memory",
			"has": {
				"cordova": {
					"type": "dcordova/ContactsStore"
				}
			},
			"observable": true,
			"params": {
				"data": [
					{
						"id": "1",
						"displayName": "Ed Chatelain",
						"name": {
							"givenName": "Edward",
							"familyName": "Chatelain"
						},
						"emails": [
							{
								"type": "home",
								"value": "me@myhome.com"
							}
						],
						"phoneNumbers": [
							{
								"type": "home",
								"value": "10-20-30-40"
							},
							{
								"type": "work",
								"value": "11-22-33-44"
							}
						],
						"organizations": []
					},
					{
						"id": "2",
						"displayName": "Christophe Jolif",
						"name": {
							"givenName": "Christophe",
							"familyName": "Jolif"
						},
						"emails": [
							{
								"type": "home",
								"value": "him@myhome.com"
							},
							{
								"type": "work",
								"value": "dev@dev.com"
							}
						],
						"phoneNumbers": [
							{
								"type": "home",
								"value": "11-43-81-63-19"
							},
							{
								"type": "work",
								"value": "11-48-07-13-38"
							}
						],
						"organizations": []
					}
				]
			}
		}
	},
	"views": {
		"list": {
			"controller": "demo/list",
			"template": "demo/list.html",
			"nls": "demo/nls/list",
			"has": {
				"!phone": {
					"constraint": "left"
				}
			}
		},
		"detail": {
			"controller": "demo/detail",
			"template": "demo/detail.html",
			"nls": "demo/nls/detail"
		}
	},
	"has": {
		"html5history": {
			"controllers": [
				"dojox/app/controllers/History"
			]
		},
		"!html5history": {
			"controllers": [
				"dojox/app/controllers/HistoryHash"
			]
		},
		"!cordova": {
			"dependencies": [
				"dojo/store/Memory"
			]
		},
		"cordova": {
			"dependencies": [
				"dcordova/ContactsStore"
			]
		}
	}
}
